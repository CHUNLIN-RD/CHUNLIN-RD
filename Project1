C:
main()
{
  int i,sum=0;
  for(i=1;i<=10;i++)
  {
    sum=sum+i;
  }
}

RISC-V:

main: add x5,x0,x0
      addi x6,x0,1
loop: slti x7,x6,11
      beq x7,x0,EXIT #beq x7 x0 16
      add x5,x5,x6
      addi x6,x6,1
      j loop         #jal x0 -16(ffff0)1111 1111 1111 1111 0000
EXIT:

machine code:

